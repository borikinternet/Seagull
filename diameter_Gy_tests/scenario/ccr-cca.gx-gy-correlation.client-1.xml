<scenario>

    <counter>
        <counterdef name="HbH-counter" init="1000"></counterdef>
        <counterdef name="EtE-counter" init="2000"></counterdef>
        <counterdef name="session-counter" init="1000"></counterdef>
        <counterdef name="charging-id-counter" init="1000"></counterdef>
    </counter>

    <correlation>
        <channel name="channel-1">
            <retrieve>
                <search-in-map name="Session-Id"></search-in-map>
                <search-in-map name="EtE-id"></search-in-map>
            </retrieve>

            <command name="gx">
                <pre-action>
                    <inc-counter name="session-counter"></inc-counter>
                    <set-value name="Session-Id"
                               format="seagull.airlinq.net;1096298391;$(session-counter)"></set-value>
                </pre-action>
                <post-action>
                    <store name="SID" entity="Session-Id"></store>
                    <insert-in-map channel="channel-1" name="SID"></insert-in-map>
                </post-action>
            </command>

            <command name="gy">
                <pre-action>
                    <inc-counter name="session-counter"></inc-counter>
                    <set-value name="Session-Id"
                               format="seagull.airlinq.net;1096298391;$(session-counter)"></set-value>
                </pre-action>
                <post-action>
                    <store name="SID1" entity="Session-Id"></store>
                    <insert-in-map channel="channel-1" name="SID1"></insert-in-map>
                </post-action>
            </command>
        </channel>
    </correlation>

    <init>
        <send channel="channel-1">
            <action>
                <inc-counter name="HbH-counter"></inc-counter>
                <inc-counter name="EtE-counter"></inc-counter>
                <set-value name="HbH-id" format="$(HbH-counter)"></set-value>
                <set-value name="EtE-id" format="$(EtE-counter)"></set-value>
            </action>
            <command name="CER">
                <avp name="Origin-Host" value="seagull-1.airlinq.net"></avp>
                <avp name="Origin-Realm" value="airlinq.net"></avp>
                <avp name="Host-IP-Address" value="0x00010adf487c"></avp>
                <avp name="Vendor-Id" value="10415"></avp>
                <avp name="Product-Name" value="SeagullTesting"></avp>
                <avp name="Supported-Vendor-Id" value="10415"></avp>
                <avp name="Vendor-Specific-Application-Id">
                    <avp name="Vendor-Id" value="10415"></avp>
                    <avp name="Auth-Application-Id" value="4"></avp>
                </avp>
                <avp name="Vendor-Specific-Application-Id">
                    <avp name="Vendor-Id" value="10415"></avp>
                    <avp name="Auth-Application-Id" value="16777238"></avp>
                </avp>
                <avp name="Firmware-Revision" value="1"></avp>
            </command>
        </send>

        <receive channel="channel-1">
            <command name="CEA">
                <avp name="Result-Code" value="2001"></avp>
            </command>
<!--
            <action>
                <check-value name="Result-Code" behaviour="error"></check-value>
            </action>
-->
        </receive>
    </init>

    <default behaviour="ignore">
        <receive channel="channel-1">
            <command name="DWR"></command>
            <action>
                <store name="HBH_ID" entity="HbH-id"></store>
                <store name="ETE_ID" entity="EtE-id"></store>
            </action>
        </receive>

        <send channel="channel-1">
            <action>
                <restore name="HBH_ID" entity="HbH-id"></restore>
                <restore name="ETE_ID" entity="EtE-id"></restore>
            </action>
            <command name="DWA">
                <avp name="Result-Code" value="2001"></avp>
                <avp name="Origin-Host" value="seagull.airlinq.net"></avp>
                <avp name="Origin-Realm" value="airlinq.net"></avp>
                <avp name="Origin-State-Id" value="0"></avp>
            </command>
        </send>
    </default>

